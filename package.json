{
  "name": "vite-react-typescript",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "eCommerce-Application",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EvgenKlo/eCommerce-Application"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "tsc --noEmit && eslint ./src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint ./src --ext ts,tsx --fix",
    "preview": "vite preview",
    "prepare": "husky install",
    "lintstaged": "lint-staged --relative",
    "style": "npx stylelint 'src/**/*.css'",
    "test": "vitest",
    "coverage": "npx vitest --coverage"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.css": [
      "prettier --write -config .prettierrc"
    ],
    "**/*.scss": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "@commercetools/platform-sdk": "^4.11.0",
    "@commercetools/sdk-client-v2": "^2.2.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@fontsource/roboto": "^5.0.7",
    "@mui/icons-material": "^5.14.3",
    "@mui/lab": "^5.0.0-alpha.140",
    "@mui/material": "^5.14.3",
    "@mui/x-date-pickers": "^6.11.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/react-image-gallery": "^1.2.0",
    "compose-function": "^3.0.3",
    "date-fns": "^2.30.0",
    "isemail": "^3.2.0",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-image-gallery": "^1.3.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.14.1",
    "rollup-plugin-node-polyfills": "^0.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/compose-function": "^0.0.30",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-image-gallery": "^1.2.0",
    "@types/react-redux": "^7.1.25",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-c8": "^0.33.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.3",
    "netlify-cli": "^16.0.3",
    "prettier": "^3.0.0",
    "sass": "^1.63.6",
    "stylelint": "^15.10.2",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-scss": "^5.1.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.4",
    "vite-plugin-node-polyfills": "^0.11.0",
    "vitest": "^0.33.0"
  }
}
